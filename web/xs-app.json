{
	"welcomeFile": "/web/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/web/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "^/api(.*)",
		"target": "$1",
		"destination": "api",
		"csrfProtection": true
	}]
}