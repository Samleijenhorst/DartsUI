<mvc:View controllerName="dartsui5.web.web.controller.DartsUI" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="DartsUI">
					<content>
						<List
							id="OptionsTable"
							items="{path: 'DartsModel>/'}">
							<headerToolbar>
								<Toolbar>
									<SearchField
										id="Search"
										width=""
										placeholder="Huidige score..."
										showSearchButton="true"
										search="onChange"
										liveChange="onChange"/>
								</Toolbar>
							</headerToolbar>
							<items>
								<StandardListItem
									title="{DartsModel>value}"
									highlight="{path: 'DartsModel>value', formatter: '.formatter.colour'}">
								</StandardListItem>
							</items>
						</List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>